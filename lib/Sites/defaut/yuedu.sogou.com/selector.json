{
  "infoPage": {
    "match": "!!$('.detail-info').length",
    "indexPage": "$.location().replace(/\\?.*/g,'').replace('book/info','ajax/book/info')",
    "footer": "$('#footer').length > 0",
    "bookInfos": {
      "origin": "$.location()",
      "source": "$.location()",
      "title": "$('h1.detail-tit').text()",
      "author": "$('td:contains(作者：)').text()",
      "classes": "$('td:contains(类型：)').text()",
      "isend": "$('td:contains(状态：)').text()",
      "cover": "$.location($('.detail-thumb-box img').attr('src'))",
      "brief": "$('#info_long_txt').html()"
    }
  },
  "indexPage": {
    "match": "/ajax\\/book\\/info/i.test($.location())",
    "infoPage": "$.location().replace('ajax/book/info','book/info')",
    "footer": "true",
    "bookIndexs": "this.match($) && JSON.parse($.html()).dir.chapters.filter(v=>!v.lock).map(v=>({href:v.href.replace(/\\/book\\/view\\/(\\w+)\\/(\\w+)/,'http://yuedu.sogou.com/ajax/user/buy/?bkey=$1&ckey=$2'),text:v.name}))"
  },
  "contentPage": {
    "match": "/ajax\\/user\\/buy/i.test($.location())",
    "footer": "true",
    "chapterInfos": {
      "source": "$.location()",
      "content": "/user\\/buy/i.test($.location()) && JSON.parse($.html()).detail.content"
    }
  }
}