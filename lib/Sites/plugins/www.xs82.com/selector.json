{
  "infoPage": {
    "match": "/^http:\\/\\/www\\.xs82\\.com\\/book\\/\\d+\\/index\\.html$/i.test($.location())",
    "indexPage": "$.location($('.btopt a').attr('href'))",
    "footer": "$('.footers').length > 0",
    "filter": "$('.intro a').remove()",
    "bookInfos": {
      "source": "$.location()",
      "title": "$('h1.f20h').text()",
      "author": "$('h1.f20h em').text()",
      "classes": "$('tr[valign=\"top\"]').find('td').eq(0).text()",
      "isend": "$('tr[valign=\"top\"]').find('td').eq(2).text()",
      "cover": "$.location($('.pic img').attr('src'))",
      "brief": "$('.intro').html()"
    }
  },
  "indexPage": {
    "match": "/^http:\\/\\/www\\.xs82\\.com\\/books\\/\\d+\\/\\d+\\/$/i.test($.location())",
    "infoPage": "$.location($('ul.bread-crumbs a').eq(1).attr('href'))",
    "footer": "$('#bgdiv').length > 0",
    "bookIndexs": "$('.chapterlist a').map((i,v)=>({href:$.location($(v).attr('href')),text:$(v).text()})).toArray()"
  },
  "contentPage": {
    "match": "/^http:\\/\\/www\\.xs82\\.com\\/books\\/\\d+\\/\\d+\\/\\d+\\.html$/i.test($.location())",
    "footer": "$('.botm').length > 0",
    "filter": "$('#content').find('div,script,a').remove()",
    "chapterInfos": {
      "title": "$('h2').text()",
      "source": "$.location()",
      "content": "$('#content').html()"
    }
  }
}