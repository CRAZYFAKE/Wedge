{
  "infoPage": {
    "match": "!!$('#detail').length",
    "indexPage": "$.location($('a:contains(查看目录)').attr('href'))",
    "footer": "!!$('#copyright').length",
    "bookInfos": {
      "source": "$.location()",
      "title": "$('.cont h3').eq(0).text().trim()",
      "author": "$('.info a').eq(0).text().trim()",
      "classes": "$('.info a').eq(1).text().trim()",
      "isend": "$('.info').text()",
      "cover": "$.location($('.pic img').attr('src'))",
      "brief": "$('#full-intro').html()"
    }
  },
  "indexPage": {
    "match": "/xiaoshuo.360.cn\\/chaplist/i.test($.location())",
    "infoPage": "$.location($('#crumb a').eq(-1).attr('href'))",
    "footer": "!!$('#doc-ft').length",
    "filter": "$('.vip').remove()",
    "bookIndexs": "$('.tlist > li > a').map((i,v)=>({href:$.location($(v).attr('href')),text:$(v).text()})).toArray()"
  },
  "contentPage": {
    "match": "/xiaoshuo.360.cn\\/chapter/i.test($.location())",
    "footer": "true",
    "chapterInfos": {
      "source": "$.location()",
      "content": "new Buffer($('#feedback').nextAll('script').eq(2).text().replace(/var c=\"(.*)?\";/,'$1'),'base64').toString()"
    }
  }
}